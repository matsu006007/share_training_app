<h1>Tweet投稿</h1>

<%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
<%= link_to "Tweet一覧へ戻る", tweets_path %>

<%= form_with model: @tweet, local: true do |f| %>
  <h3>ジャンルを選んでください</h3>
    <%= f.text_field :genre_id %>
  <h3>内容</h3>
    <%= f.text_area :content %>
    <%= f.submit '投稿する'%>
<% end%>

